<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="@dimen/dp_296"
    android:layout_height="wrap_content"
    android:background="@drawable/shape_white_rounded"
    android:orientation="vertical">

    <TextView
        android:id="@+id/tv_dialog_standard_twobutton_content"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:paddingLeft="@dimen/dp_14"
        android:paddingTop="@dimen/dp_34"
        android:paddingRight="@dimen/dp_14"
        android:paddingBottom="@dimen/dp_34"
        android:text=""
        android:textColor="@android:color/black"
        android:textSize="@dimen/sp_16"
        android:textStyle="bold" />


    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_1"
        android:background="@color/personal_line_gray" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_56"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/tv_dialog_standard_twobutton_leftbutton"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="@drawable/selector_white_gray_0_0_0_5"
            android:gravity="center"
            android:text=""
            android:textColor="@color/txtblack"
            android:textSize="@dimen/sp_16" />

        <View
            android:layout_width="@dimen/dp_1"
            android:layout_height="match_parent"
            android:background="@color/personal_line_gray" />

        <TextView
            android:id="@+id/tv_dialog_standard_twobutton_rightbutton"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="@drawable/selector_white_gray_0_0_5_0"
            android:gravity="center"
            android:text=""
            android:textColor="@color/lightblue"
            android:textSize="@dimen/sp_16" />
    </LinearLayout>


</LinearLayout>